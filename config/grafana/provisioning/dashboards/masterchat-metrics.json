{
  "uid": "masterchat-metrics",
  "title": "MasterChat Metrics",
  "tags": ["masterchat", "metrics"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Task Queue Depth",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "task_queue_depth",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        },
        "overrides": []
      },
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 0}
    },
    {
      "type": "timeseries",
      "title": "Tasks Executed Total (by type)",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "tasks_executed_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 6}
    }
  ]
} 