{
  "uid": "crawler-metrics",
  "title": "Crawler Metrics",
  "tags": ["crawler", "metrics"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "15s",
  "panels": [
    {
      "type": "stat",
      "title": "Concepts Crawled Total",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "concepts_crawled_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        },
        "overrides": []
      },
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 0}
    },
    {
      "type": "timeseries",
      "title": "Crawl Latency (seconds)",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "crawl_latency_seconds",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 2
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 6}
    }
  ]
} 