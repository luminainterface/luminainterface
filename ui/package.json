{
  "name": "lumina-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:headless": "cypress run --browser chrome",
    "cypress:ci": "cypress run --browser chrome --reporter mochawesome --config screenshotOnRunFailure=true",
    "cypress:stubbed": "cypress run --spec cypress/e2e/wiki_qa.cy.ts",
    "cypress:live": "cypress run --spec cypress/e2e/wiki_qa_live.cy.ts",
    "cypress:smoke": "cypress run --spec cypress/e2e/smoke.cy.ts"
  },
  "dependencies": {
