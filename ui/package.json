    "axios": "^1.6.7",
    "canvas-confetti": "^1.9.3",
    "chart.js": "^4.4.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "d3": "^7.8.5",
    "date-fns": "^4.1.0",
    "mitt": "^3.0.1",
    "pinia": "^2.1.0",
    "pixi.js": "^7.4.0",
    "postcss": "^8.4.35",
